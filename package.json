    "express": "^4.16.3",
    "forever": "^0.15.3",
    "lodash": "^4.17.11",
    "log-timestamp": "^0.2.1",
    "mongodb": "^3.1.6",
    "mongoose": "^5.3.1",
    "querystring": "^0.2.0",
    "rand-token": "^0.4.0",
    "request": "^2.88.0",
