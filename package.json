    "@slack/events-api": "^1.0.1",
    "@slack/interactive-messages": "^0.4.0",
    "await-timeout": "^0.3.0",
    "body-parser": "^1.18.3",
    "botkit": "^0.6.16",
    "botkit-storage-mongo": "^1.0.7",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
